            </div>

            <div style="height: 50px" data-ng-if="dnnsf.isIframe">
                <nav class="navbar navbar-inverse navbar-fixed-bottom" id="navBottom" role="navigation">
                    <div class="navbar-inner">
                        <ul class="nav navbar-nav navbar-right" style="margin-right: 20px;">
                            <li><a href="<%# ViewUrl %>" target="_top" style="font-weight: bold; color: #fff;" title="Return to the form. Make sure to save your settings first.">Back</a></li>
                            <li><a href="<%# Request.RawUrl.Replace("mode=iframe", "mode=full") %>" target="_parent" title="Open the admin in a separate window." class="visible-in-iframe"><i class="glyphicon glyphicon-resize-full"></i></a></li>
                        </ul>

                        <button type="button" class="btn btn-warning navbar-btn pull-right" data-ng-click="save()" data-ng-if="modified && !saving" data-ng-disabled="saveDisabled">Save</button>
                        <button type="button" class="btn btn-link navbar-btn pull-right disabled" data-ng-if="!modified && !saving">Saved</button>
                        <button type="button" class="btn btn-link navbar-btn pull-right disabled" data-ng-if="saving">Saving</button>



                        <ul class="nav navbar-nav navbar-left">
                            <li><a href="#General">General</a></li>
