[{
    "Id": "UpdateRole",
    "Title": {
        "default": "Update Role"
    },
    "TypeStr": "DnnSharp.Common.Actions.Role.UpdateRole, DnnSharp.Common.Actions",
    "HelpText": {
        "default": "Updates a role on the current portal"
    },
    "Settings": {
        "Group": "Role"
    },
    "Parameters": [{
            "Id": "RoleIdentifier",
            "Title": {
                "default": "Role Identifier"
            },
            "HelpText": {
                "default": "The identifier of the role that will be updated. You can use a role name or a role ID. Can contain <a href=\"http://www.dnnsharp.com/dnn/modules/my-custom-tokens\" target=\"_blank\">My Tokens</a>."
            },
            "Type": "Text"
        },
        {
            "Id": "RoleGroupIdentifier",
            "Title": {
                "default": "Role Group Identifier"
            },
            "HelpText": {
                "default": "The identifier of the group that will be assigned to the role. You can use a group name or a group ID, nothing changes if no input is received. Can contain <a href=\"http://www.dnnsharp.com/dnn/modules/my-custom-tokens\" target=\"_blank\">My Tokens</a>."
            },
            "Type": "Text"
        },
        {
            "Id": "NewRoleName",
            "Title": {
                "default": "Role Name"
            },
            "HelpText": {
                "default": "The new name of the role, nothing changes if no input is received. Can contain <a href=\"http://www.dnnsharp.com/dnn/modules/my-custom-tokens\" target=\"_blank\">My Tokens</a>."
            },
            "Type": "Text"
        },
        {
            "Id": "RoleDescription",
            "Title": {
                "default": "Role Description"
            },
            "HelpText": {
                "default": "The new description of the role, nothing changes if no input is received. Can contain <a href=\"http://www.dnnsharp.com/dnn/modules/my-custom-tokens\" target=\"_blank\">My Tokens</a>."
            },
            "Type": "Text"
        },
        {
            "Id": "RoleStatus",
            "Title": {
                "default": "Role Status"
            },
            "HelpText": {
                "default": "The new status of the role. If you use an expression, the values accepted are -1 (Pending), 0 (Disabled), 1 (Approved), nothing changes if no input is received. Can contain <a href=\"http://www.dnnsharp.com/dnn/modules/my-custom-tokens\" target=\"_blank\">My Tokens</a>."
            },
            "Type": "Select",
            "Settings": {
                "Items": {
                    "-1": {
                        "default": "Pending"
                    },
                    "0": {
                        "default": "Disabled"
                    },
                    "1": {
                        "default": "Approved"
                    }
                },
                "SupportsExpressions": "true"
            }
        },
        {
            "Id": "AddToExistingUsers",
            "Title": {
                "default": "Add to existing users"
            },
            "HelpText": {
                "default": "Add the updated role to existing users. A value of \"true\" or  \"false\" should be provided, nothing changes if no input is received. Can contain <a href=\"http://www.dnnsharp.com/dnn/modules/my-custom-tokens\" target=\"_blank\">My Tokens</a>."
            },
            "Type": "Text"
        },
        {
            "Id": "IsPublic",
            "Title": {
                "default": "Is Public"
            },
            "HelpText": {
                "default": "Is a public role. A value of \"true\" or  \"false\" should be provided, nothing changes if no input is received. Can contain <a href=\"http://www.dnnsharp.com/dnn/modules/my-custom-tokens\" target=\"_blank\">My Tokens</a>."
            },
            "Type": "Text"
        },
        {
            "Id": "AutoAssign",
            "Title": {
                "default": "Auto Assign"
            },
            "HelpText": {
                "default": "Add the updated role to all users that will register on the portal. A value of \"true\" or  \"false\" should be provided, if no input is received the role will not be added to already existing users. Can contain <a href=\"http://www.dnnsharp.com/dnn/modules/my-custom-tokens\" target=\"_blank\">My Tokens</a>."
            },
            "Type": "Text"
        }
    ]
}]