[{
    "For": "AGRID,AFORM",
    "Id": "DowndloadFile",
    "Title": {
        "default": "Send Plain Text as File Download"
    },
    "HelpText": {
        "default": "Takes the specified plain text contents and passes them to the browser for user download."
    },
    "TypeStr": "DnnSharp.Common.Actions.Message.DowndloadFile, DnnSharp.Common.Actions",
    "Settings": {
        "Group": "Message"
    },
    "Parameters": [{
            "Id": "FileName",
            "Title": {
                "default": "File name"
            },
            "Type": "Text",
            "HelpText": {
                "default": "Optionally provide name for the file. Leave blank to use current page name. Supports <a href=\"http://www.dnnsharp.com/dnn/modules/my-custom-tokens\" target=\"_blank\" tabindex=\"-1\">My Tokens</a>"
            }
        },
        {
            "Id": "FileTypeId",
            "Title": {
                "default": "File Type"
            },
            "Type": "Select",
            "HelpText": {
                "default": "This is the content type of the file. Supports <a href=\"http://www.dnnsharp.com/dnn/modules/my-custom-tokens\" target=\"_blank\" tabindex=\"-1\">My Tokens</a>"
            },
            "Settings": {
                "Items": "{ \"csv\": { \"default\": \"csv (text/csv)\" }, \"html\": { \"default\": \"html (text/html)\" }, \"txt\": { \"default\": \"txt (text/plain)\" }, \"xml\": { \"default\": \"xml (text/xml)\" }, \"xsl\": { \"default\": \"xsl (text/xml)\" }}",
                "SupportsExpressions": "true"
            }
        },
        {
            "Id": "Headers",
            "Title": {
                "default": "Headers"
            },
            "Type": "NameValueCollection",
            "HelpText": {
                "default": "Additional HTTP headers to pass"
            },
            "Settings": {
                "NameColumn": "Name",
                "ValueColumn": "Value",
                "ButtonName": "Add"
            }
        },
        {
            "Id": "FileCheck",
            "Title": {
                "default": "Force download"
            },
            "Type": "Boolean",
            "HelpText": {
                "default": "Check if you want the browser to automatically start the file download."
            }
        },
        {
            "Id": "DowndloadFile",
            "Title": {
                "default": "File Content"
            },
            "Type": "Textarea",
            "HelpText": {
                "default": "Specify. Supports <a href=\"http://www.dnnsharp.com/dnn/modules/my-custom-tokens\" target=\"_blank\" tabindex=\"-1\">My Tokens</a>"
            }
        }
    ]
}]